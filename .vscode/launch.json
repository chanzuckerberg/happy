    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug stack create",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "create",
                "--project-root",
                "~/Documents/single-cell-data-portal",
                "jheath"
            ],
        },
        {
            "name": "Log stack",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "logs",
                "-v",
                "--project-root",
                "~/Documents/single-cell-data-portal",
                "--since",
                "1m",
                "ebezzi-test", 
                "ebezzi-test-backend"
            ],
        }
    ]
}