{
    "config_version": "v3",
    "default_env": "rdev",
    "app": "base",
    "default_compose_env_file": ".env.ecr",
    "slice_default_tag": "branch-main",
    "services": [
        "base"
    ],
    "environments": {
        "rdev": {
            "aws_profile": "czi-si",
            "k8s":{
                "namespace": "k8s-test-app-rdev-happy-happy-env",
                "cluster_id": "k8s-test-app-rdev-eks",
                "auth_method": "eks",
                "context": "k8s-test-app-rdev-eks"
            },
            "terraform_directory": ".happy/terraform/envs/rdev",
            "task_launch_type": "k8s",
            "auto_run_migrations": false
        },
        "prod": {
            "aws_profile": "czi-playground",
            "k8s":{
                "namespace": "si-playground-happy-happy-env",
                "cluster_id": "si-playground-eks",
                "auth_method": "eks",
                "context": "si-playground-eks"
            },
            "terraform_directory": ".happy/terraform/envs/prod",
            "task_launch_type": "k8s",
            "auto_run_migrations": false
        }
    },
    "features": {
        "enable_happy_api_usage": true,
        "enable_ecr_auto_creation": true
    },
    "tasks": {}
}
